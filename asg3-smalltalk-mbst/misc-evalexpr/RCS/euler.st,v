@#!/afs/cats.ucsc.edu/courses/cse112-wm/usr/smalltalk/bin/gst -f
"$Id: euler.st,v 1.1 2021-02-18 17:09:36-08 - - $"

i := -1 sqrt.
pi := -1 arcCos.
euler := (i * pi) exp + 1.

nl := Character nl.
stdout << 'i = ' << i << nl.
stdout << 'pi = ' << pi << nl.
stdout << 'euler = ' << euler << nl.

@
